./gem5/build/X86/gem5.opt -d checkpoints/canneal --stats-file=$1 gem5/configs/example/se.py --cpu-type=detailed --mem-type=SimpleMemory --cmd=./apps/x86/parsec/canneal/canneal --options="1 10000 2000 apps/x86/parsec/canneal/100000.nets" --checkpoint-dir=checkpoints/canneal -r 1 --caches -I 100000000 --restore-with-cpu=detailed  --caches -I 100000000 --l2cache --issue_width=$2 --l2_size=$3kB --l1d_size=$4kB --l1i_size=$4kB --l1d_assoc=$5 --l1i_assoc=$5 --reorder_buffer=$6 --int_alu=$7 --int_mul=$8 --fp=$9

